<configuration>

  <appender name="FILE" class="ch.qos.logback.core.FileAppender">
    <encoder>
      <pattern>[%-5level] %d{HH:mm:ss.SSS} %logger{36} - %msg%n</pattern>
    </encoder>
    <file>logs/test.log</file>
  </appender>

  <root level="debug">
    <appender-ref ref="FILE" />
  </root>
  
</configuration>